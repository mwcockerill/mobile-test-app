{
  "name": "mobile-test-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "e2e:build": "detox build --configuration ios.sim.debug",
    "e2e:test": "detox test --configuration ios.sim.debug",
    "e2e:build:android": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd - && mkdir -p e2e-build && cp android/app/build/outputs/apk/debug/app-debug.apk e2e-build/ && cp android/app/build/outputs/apk/androidTest/debug/app-debug-androidTest.apk e2e-build/",
    "e2e:test:android": "source env.android.zsh && detox test -c android.emu.debug --record-logs all --take-screenshots failing",
    "ci:build": "npm ci && npm run e2e:build:android",
    "ci:test": "detox test --configuration android.emu.debug --cleanup --headless --record-logs all --take-screenshots failing",
    "ci:test:debug": "detox test --configuration android.emu.debug --cleanup --record-logs all --take-screenshots all --record-videos failing"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "detox": "^20.40.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "detox": {
    "testRunner": "jest",
    "runnerConfig": "e2e/jest.config.js",
    "apps": {
      "android.debug": {
        "type": "android.apk",
        "binaryPath": "e2e-build/app-debug.apk",
        "testBinaryPath": "e2e-build/app-debug-androidTest.apk"
      }
    },
    "devices": {
      "android.real": {
        "type": "android.attached"
      },
      "android.emulator": {
        "type": "android.emulator",
        "device": {
          "avdName": "test_emulator"
        }
      }
    },
    "configurations": {
      "android.debug.real": {
        "device": "android.real",
        "app": "android.debug"
      },
      "android.emu.debug": {
        "device": "android.emulator",
        "app": "android.debug"
      }
    }
  }
}
